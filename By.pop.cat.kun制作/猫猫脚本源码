        if input.KeyCode == Enum.KeyCode.Q then
            print("l")
            local MagClone = SCARH.Mag:Clone()
            MagClone.Parent = workspace
            MagClone.CFrame = SCARH.Mag.CFrame
            MagClone.ManualWeld:Destroy()
            Magclone.CanCollide = true
            task.wait(10)
            MagClone:Destroy()
        end
    end
end)
  	end
})

