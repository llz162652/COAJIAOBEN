    Cham.DepthMode = Enum.HighlightDepthMode.AlwaysOnTop
    Cham.FillColor = Color3.new(0,0,0)
    Cham.FillTransparency = 0
    Cham.OutlineColor = Color3.new(0,0,0)
    Cham.Parent = inst
    Cham.Enabled =bai.fuces
    Cham.Adornee = inst
