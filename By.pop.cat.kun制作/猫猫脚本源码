		function()
			if minimized == false then
MinimizeIcon.Image = "http://www.roblox.com/asset/?id=10673592685"
MinimizeBtn.Position = UDim2.new(0.417947114, 0, -0.0169996787, 0)
CloseBtn.Position = UDim2.new(0.459063113, 0, -0.0169996787, 0)
				MainFrame:TweenSize(
					UDim2.new(0, 340, 0, 22),
					Enum.EasingDirection.Out,
					Enum.EasingStyle.Quart,
					.3,
					true
				)
			else
MinimizeIcon.Image = "http://www.roblox.com/asset/?id=6035067836"
MinimizeBtn.Position = UDim2.new(0.917947114, 0, -0.0169996787, 0)
CloseBtn.Position = UDim2.new(0.959063113, 0, -0.0169996787, 0)
				MainFrame:TweenSize(
					UDim2.new(0, 681, 0, 396),
					Enum.EasingDirection.Out,
					Enum.EasingStyle.Quart,
					.3,
					true
				)
			end
			minimized = not minimized
		end
	)
	
